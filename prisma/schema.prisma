generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Elixir {
  Username String?
  id       Int     @id
}


model User {
  id       Int @id @default(autoincrement())
  email String @unique
  username String @unique
  password String
  created_at DateTime @default(now())
}